<tr><td>Total:</td><td id="ttl">0</td></tr>
</table>
<script type="text/javascript">
$(function() {
	viewerCount();
});
function viewerCount() {
	$.getJSON("//bronytv.net/viewers.php", function(viewers) {
		viewers = viewers[0].Total_Viewers;
		$('#ttl').html(viewers);
	});
	setTimeout('viewerCount()', 1000);
};
